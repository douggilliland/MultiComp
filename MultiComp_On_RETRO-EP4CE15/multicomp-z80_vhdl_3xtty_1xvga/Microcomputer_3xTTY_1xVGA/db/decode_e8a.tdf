--synthesis_resources = lut 4 
SUBDESIGN decode_e8a
( 
	data[1..0]	:	input;
	eq[2..0]	:	output;
) 
VARIABLE 
	data_wire[1..0]	: WIRE;
	eq_node[2..0]	: WIRE;
	eq_wire[3..0]	: WIRE;
	w_anode381w[2..0]	: WIRE;
	w_anode395w[2..0]	: WIRE;
	w_anode404w[2..0]	: WIRE;
	w_anode413w[2..0]	: WIRE;

BEGIN 
	data_wire[] = data[];
	eq[] = eq_node[];
	eq_node[2..0] = eq_wire[2..0];
	eq_wire[] = ( w_anode413w[2..2], w_anode404w[2..2], w_anode395w[2..2], w_anode381w[2..2]);
	w_anode381w[] = ( (w_anode381w[1..1] & (! data_wire[1..1])), (w_anode381w[0..0] & (! data_wire[0..0])), B"1");
	w_anode395w[] = ( (w_anode395w[1..1] & (! data_wire[1..1])), (w_anode395w[0..0] & data_wire[0..0]), B"1");
	w_anode404w[] = ( (w_anode404w[1..1] & data_wire[1..1]), (w_anode404w[0..0] & (! data_wire[0..0])), B"1");
	w_anode413w[] = ( (w_anode413w[1..1] & data_wire[1..1]), (w_anode413w[0..0] & data_wire[0..0]), B"1");
END;
