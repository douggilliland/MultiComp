FUNCTION sign_div_unsign_bnh (denominator[14..0], numerator[14..0])
RETURNS ( quotient[14..0], remainder[14..0]);

--synthesis_resources = 
SUBDESIGN lpm_divide_lcm
( 
	denom[14..0]	:	input;
	numer[13..0]	:	input;
	quotient[13..0]	:	output;
	remain[14..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_bnh;
	gnd_wire	: WIRE;
	num_padder[0..0]	: WIRE;
	numer_tmp[14..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	gnd_wire = B"0";
	num_padder[] = ( gnd_wire);
	numer_tmp[] = ( num_padder[], numer[]);
	quotient[13..0] = divider.quotient[13..0];
	remain[] = divider.remainder[];
END;
